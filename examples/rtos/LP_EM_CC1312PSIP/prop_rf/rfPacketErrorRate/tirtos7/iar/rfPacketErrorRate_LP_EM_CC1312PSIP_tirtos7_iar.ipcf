        <path copyTo="$PROJ_DIR$/tx.c">$COM_TI_SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/examples/rtos/LP_EM_CC1312PSIP/prop_rf/rfPacketErrorRate/tx.c</path>
    </files>
</iarProjectConnection>